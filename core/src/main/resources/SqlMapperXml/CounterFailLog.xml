<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ifhz.core.mapper.CounterFailLogMapper">

    <resultMap id="CounterUploadLogResultMap" type="com.ifhz.core.po.CounterFailLog">
        <id column="fail_id" property="failId" jdbcType="BIGINT"/>
        <result column="imei" property="imei" jdbcType="VARCHAR"/>
        <result column="ua" property="ua" jdbcType="VARCHAR"/>
        <result column="model_name" property="modelName" jdbcType="VARCHAR"/>
        <result column="channel_id" property="channelId" jdbcType="VARCHAR"/>
        <result column="device_code" property="deviceCode" jdbcType="VARCHAR"/>
        <result column="batch_code" property="batchCode" jdbcType="VARCHAR"/>
        <result column="group_id" property="groupId" jdbcType="BIGINT"/>
        <result column="active" property="active" jdbcType="VARCHAR"/>
        <result column="process_time" property="processTime" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="CommonFieldSql">
        FAIL_ID,
        IMEI,
        UA,
        MODEL_NAME,
        CHANNEL_ID,
        DEVICE_CODE,
        GROUP_ID,
        BATCH_CODE,
        PROCESS_TIME,
        CREATE_TIME,
        ACTIVE
    </sql>
    <sql id="TABLE_NAME">TY_COUNTER_FAIL_LOG</sql>

    <insert id="insert" parameterType="com.ifhz.core.po.CounterFailLog">
        <selectKey resultType="java.lang.Long" keyProperty="failId" order="AFTER">
            SELECT SEQ_COUNTER_FAIL_LOG.CURRVAL FROM DUAL
        </selectKey>
        insert into
        <include refid="TABLE_NAME"/>
        (
        <include refid="CommonFieldSql"/>
        )
        values
        (
        SEQ_TY_COUNTER_FAIL_LOG.NEXTVAL,
        #{imei,jdbcType=VARCHAR},
        #{ua,jdbcType=VARCHAR},
        #{modelName,jdbcType=VARCHAR},
        #{channelId,jdbcType=VARCHAR},
        #{deviceCode,jdbcType=VARCHAR},
        #{groupId,jdbcType=BIGINT},
        #{batchCode,jdbcType=VARCHAR},
        #{processTime,jdbcType=VARCHAR},
        #{createTime,jdbcType=TIMESTAMP},
        #{active,jdbcType=VARCHAR}
        )
    </insert>

    <select id="queryTotalCount">
        select count(0)
        from
        <include refid="TABLE_NAME"/>
        where create_time between #{startTime} and #{endTime}
    </select>

    <select id="queryPage">
        select count(0)
        from
        <include refid="TABLE_NAME"/>
        where create_time between #{startTime} and #{endTime}
    </select>

    <delete id="delete">
        delete from
        <include refid="TABLE_NAME"/>
        where FAIL_ID = #{failId}
    </delete>
</mapper>