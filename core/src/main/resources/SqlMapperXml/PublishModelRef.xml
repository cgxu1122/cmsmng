<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ifhz.core.mapper.PublishModelRefMapper">
    <!--

    -->
    <sql id="TABLE_NAME">TY_PUBLISH_MODEL_REF</sql>
    <sql id="CommonFieldSql">
        ID,
        PUBLISH_ID,
        PACKAGE_ID,
        MODEL_ID,
        CREATE_TIME
    </sql>

    <resultMap id="PublishModelRefResultMap" type="com.ifhz.core.po.PublishModelRef">
        <id column="ID" property="apkId" jdbcType="BIGINT"/>
        <result column="PUBLISH_ID" property="publishId" jdbcType="BIGINT"/>
        <result column="PACKAGE_ID" property="packageId" jdbcType="BIGINT"/>
        <result column="MODEL_ID" property="modelId" jdbcType="BIGINT"/>
        <result column="CREATE_TIME" property="createTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <insert id="insert" keyProperty="id" parameterType="com.ifhz.core.po.PublishModelRef">
        <selectKey resultType="long" keyProperty="id">
            SELECT SEQ_PUBLISH_MODEL_REF.CURRVAL FROM DUAL
        </selectKey>
        insert into
        <include refid="TABLE_NAME"/>
        (
        <include refid="CommonFieldSql"/>
        )
        values
        (
        SEQ_PUBLISH_MODEL_REF.NEXTVAL,
        #{publishId},
        #{packageId},
        #{modelId},
        #{createTime}
        )
    </insert>
</mapper>