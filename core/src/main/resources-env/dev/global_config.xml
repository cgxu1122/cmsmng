<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>全局配置,放在此xml中</comment>
    <entry key="ftp.server.url">127.0.0.1</entry>
    <entry key="ftp.server.port">21</entry>
    <entry key="ftp.server.username">ftp</entry>
    <entry key="ftp.server.password">ftp</entry>
    <entry key="ftp.server.deviceDir">/data/device/{0}/</entry>
    <entry key="ftp.server.apkDir">/data/apk/{0}/</entry>
    <entry key="ftp.server.downloadUrl">http://127.0.0.1:8082/</entry>
    <entry key="root.path">D:\\rootpath</entry>
    <entry key="temp.path">D:\\rootpath\\temp</entry>
    <entry key="export.num.max">50000</entry>
</properties>